{
    "name": "Action Domain Responder",
    "description": "An example Action Domain Responder framework.",
    "keywords": ["adr", "slim", "php"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "slim/slim": "^3.8",
        "vlucas/valitron": "^1.4",
        "hassankhan/config": "^0.11.2"
    },
    "require-dev": {
      "kint-php/kint": "^1",
      "phpunit/php-code-coverage": "^5.2",
      "phpunit/phpunit": "^6.3",
        "mockery/mockery": "^1.1"
    },
    "autoload": {
      "psr-4": {
        "Adr\\": "src/Adr",
        "Adr\\Tests\\": "tests"
      },
      "classmap": [
        "tests"
      ]
    },
    "config": {
      "bin-dir": "bin"
    }
}
